<form class="form-horizontal form-default boxShadow">
  <div class="form-group">
    <div class="input-group dropdown-toggle" data-toggle="dropdown">
      <input type="text" class="form-control" placeholder="请选择行政区域" aria-label="" readonly="readonly" data-bind="" />
      <div class="input-group-btn">
        <button type="button" class="btn btn-default btn-angle">
          <span class="caret"></span>
        </button>
      </div>
    </div>
    <ul class="dropdown-menu dropdown-menu-default ul-dropdown-angle" data-bind="foreach: areaList">
      <li><a href="javascript:;"  data-bind="text: Description,click: $parent.selectedArea"></a></li>
    </ul>
  </div>

  <div class="form-group">
    <div class="input-group dropdown-toggle" data-toggle="dropdown">
      <input type="text" class="form-control" placeholder="年份" aria-label="" readonly="readonly" data-bind="value: nianFen" />
      <div class="input-group-btn">
        <button type="button" class="btn btn-default btn-angle">
          <span class="caret"></span>
        </button>
      </div>
    </div>
    <ul class="dropdown-menu dropdown-menu-default ul-dropdown-angle" data-bind="foreach: yearList">
      <li><a href="javascript:;" data-bind="text: $data,click: $parent.selectedYear"></a></li>
    </ul>
  </div>

  <div class="form-group">
    <input type="text" class="form-control" placeholder="关键字搜索" data-bind="value: keyWord"  />
  </div>
  <a href="javascript:;" class="pull-right promptTextColor searchFontSize  margin-bottom-10">清除搜索条件</a>
  <button type="button" class="btn btn-info btn-angle" data-bind="">搜索</button>
</form>