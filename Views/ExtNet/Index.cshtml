@using Ext.Net;
@using Ext.Net.MVC;
@{
    Layout = null;
    var X = Html.X();
}
<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" type="text/css" href=@Url.Content("~/Content/ext4/resources/css/ext-all.css") />
    <link rel="stylesheet" type="text/css" href=@Url.Content("~/Content/icon.css")  />
    <script type="text/javascript" src=@Url.Content("~/Content/ext4/ext-all-debug.js")></script>
    <script type="text/javascript" src=@Url.Content("~/Content/ext4/locale/ext-lang-zh_CN.js")></script>
    <script type="text/javascript" src=@Url.Content("~/Content/app.js")></script>
    <style type="text/css">
        .x-panel-ghost
        {
            z-index: 1;
        }
        .x-border-layout-ct
        {
            background: #DFE8F6;
        }
        .x-portal-body
        {
            padding: 0 0 0 8px;
        }
        .x-portal .x-portal-column
        {
            padding: 8px 8px 0 0;
        }
        .x-portal .x-panel-dd-spacer
        {
            border: 2px dashed #99bbe8;
            background: #f6f6f6;
            border-radius: 4px;
            -moz-border-radius: 4px;
            margin-bottom: 10px;
        }
        .x-portlet
        {
            margin-bottom: 10px;
            padding: 1px;
        }
        .x-portlet .x-panel-body
        {
            background: #fff;
        }
        .portlet-content
        {
            padding: 10px;
            font-size: 11px;
        }
    </style>
</head>
<body>
    
</body>
</html>
