        var chart1;
        var validate;

        semantic.menu = {};
        semantic.menu.ready = function() {
            var $menuItem = $('.menu a.item, .menu .link.item'),
                $dropdown = $('.menu .dropdown'),
                handler = {
                    activate: function() {
                        if (!$(this).hasClass('dropdown')) {
                            $(this)
                                .addClass('active')
                                .closest('.ui.menu')
                                .find('.item')
                                .not($(this))
                                .removeClass('active');
                        }
                    }
                };
            $dropdown.dropdown({ on: 'hover' });
            $menuItem.on('click', handler.activate);
        };


        $(document).ready(function() {

