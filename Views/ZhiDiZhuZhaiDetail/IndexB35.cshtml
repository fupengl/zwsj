                    tmpHtml = tmpHtml + '<div class="item" data-value="' + i + '">' + i.toString() + "层" + '</div>';
                    //                    if (i > 10) {
                    //                        tmpHtml = tmpHtml + '<div class="item" data-value="' + "LC" + maxLouCeng + i + '">' + i.toString() + "层" + '</div>';
                    //                        
                    //                    } else {
                    //                        tmpHtml = tmpHtml + '<div class="item" data-value="' + "LC" + maxLouCeng + "0" + i + '">' + i.toString() + "层" + '</div>';
                    //                    }

