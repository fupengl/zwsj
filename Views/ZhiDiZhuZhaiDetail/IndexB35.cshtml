                                taoCount++;
                                taoYinSu = taoYinSu + $("#jingguan").dropdown('get text').trim() + "；";
                            }
                            taoYinSu += '</label></div><br/>';
                            if (taoCount > 0) {
                                tmpHtml = tmpHtml + taoYinSu;
                            }
