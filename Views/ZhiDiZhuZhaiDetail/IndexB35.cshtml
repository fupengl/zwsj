                $("#xiaoquxinxi_tab").css("display", "block");
                $("#loudongxinxi_tab").css("display", "none");
            });
            $("#loudongxinxi").click(function() {
                $("#xiaoquxinxi_tab").css("display", "none");
                $("#loudongxinxi_tab").css("display", "block");

