                    $("#zuoluo").html(result["ZuoLuo"]);
                    $("#kaifashang").html(result["KaiFaShang"]);
                    $("#jianzaoniandai").html(result["JianZaoNianDai"]);
                    $("#jianzhumianji").html(result["JianZhuMianJi"]);
                    $("#kaipanshijian").html(result["KaiPanShiJian"]);
                    $("#jiaotongzhuangkuang").html(result["JiaoTongZhuangKuang"]);
                    $("#jianjie").html(result["JianJie"]);
                    $("#jianzhumidu").html(result["JianZhuMiDu"]);
                    $("#qitamingcheng").html(result["BeiZhu"]);
